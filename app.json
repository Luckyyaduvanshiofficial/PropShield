{
  "expo": {
    "name": "PropShield AI",
    "slug": "propshield",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "propshield",
    "userInterfaceStyle": "automatic",
    "description": "AI-powered Property Verification Platform - Verify your property in hours, not weeks",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.luckyonly.propshield",
      "infoPlist": {
        "NSCameraUsageDescription": "PropShield needs camera access to scan and capture property documents for verification.",
        "NSPhotoLibraryUsageDescription": "PropShield needs access to your photo library to select property documents for verification."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.luckyonly.propshield",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "NOTIFICATIONS"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "cc925ca8-3d2b-4beb-9b39-aee52b821672"
      }
    },
    "owner": "luckyonly",
    "updates": {
      "url": "https://u.expo.dev/cc925ca8-3d2b-4beb-9b39-aee52b821672"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
